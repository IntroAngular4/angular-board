{
  "name": "angular-board",
  "version": "7.7.3",
  "description": "Demo web application to learn Angular ;-)",
  "homepage": "https://trainingit.es/curso-angular-basico/",
  "repository": {
    "type": "git",
    "url": "https://github.com/trainingit-introangular-ed2/angular-board"
  },
  "author": {
    "name": "Alberto Basalo",
    "url": "https://twitter.com/albertobasalo"
  },
  "scripts": {
    "build:prod": "ng build --prod",
    "build:prod:schemat": "ng build schemat --prod",
    "build:pub": "ng build --prod --output-path docs --base-href https://trainingit-introangular-ed3.github.io/angular-board/",
    "e2e": "ng e2e",
    "http-server": "http-server ./dist/angular-board/ -c-1 -p4271 -a localhost -o",
    "lint": "ng lint",
    "ng": "ng",
    "pub": "npm run build:pub && npm run push",
    "push": "git add * && git commit -m 'pub' && git push",
    "start:prod": "npm run build:prod && npm run http-server",
    "start": "ng serve -o --port 4270",
    "start:schemat": "ng serve schemat -o --port 4271",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.8",
    "@angular/cdk": "~7.3.4",
    "@angular/common": "~9.0.0-next.8",
    "@angular/compiler": "~9.0.0-next.8",
    "@angular/core": "~9.0.0-next.8",
    "@angular/forms": "~9.0.0-next.8",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~9.0.0-next.8",
    "@angular/platform-browser-dynamic": "~9.0.0-next.8",
    "@angular/router": "~9.0.0-next.8",
    "core-js": "^2.5.4",
    "mini.css": "^3.0.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "~8.3.6",
    "@angular/compiler-cli": "^9.0.0-next.8",
    "@angular/language-service": "~9.0.0-next.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}